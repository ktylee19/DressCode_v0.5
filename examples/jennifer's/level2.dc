 
   //ellipse drawing methods
  e1 = ellipse(WIDTH/2,HEIGHT/2,100,100);
 
  e2 = ellipse(WIDTH/2,HEIGHT/2-80,100,200);
  e3 = ellipse(WIDTH/2-80,HEIGHT/2,200,100);
  
  e4 = ellipse(WIDTH/2,HEIGHT/2+80,100,200);
  e5 = ellipse(WIDTH/2+80,HEIGHT/2,200,100);

  ellipseGroup = group(e1,e2,e3,e4,e5);
  noFill(ellipseGroup);
  weight(ellipseGroup,3);
  stroke(ellipseGroup,PINK);
  
  