// Leaf
a=ellipse(300,300,300,400);
b=ellipse(450,300,300,400);
leaf=a&&b;
fill(leaf,69,176,48);

//create leaves
move(leaf,200,400);
rotate(leaf,90);

leaf2=copy(leaf);
move(leaf2,430,400);
rotate(leaf2,0);

leaf3=copy(leaf);
move(leaf3,300,180);
rotate(leaf3,50);

// Rose parameters
centerx=300;
centery=300;
pentSize=200;
myPoly=[];

// Rose sdf
repeat i=0:10:
  add(myPoly,poly(centerx,centery, 5,pentSize*(9/10)^i));
  fill(myPoly[i],150+8*i, 0, 0);
  rotate(myPoly[i],20*i);
end


